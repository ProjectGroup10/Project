<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.MenuButton?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox fx:id="VboxLand" prefHeight="499.0" prefWidth="660.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.MainController">
   <children>
      <AnchorPane fx:id="AnchorPane">
         <children>
            <MenuButton fx:id="Timeline" layoutX="24.0" layoutY="24.0" mnemonicParsing="false" prefHeight="45.0" prefWidth="118.0" text="Timeline">
              <items>
                <MenuItem fx:id="AddTimeline" mnemonicParsing="false" onAction="#appearFormTimeline" text="Add Timeline" />
                <MenuItem fx:id="LoadTimeline" mnemonicParsing="false" text="Load Timelines" />
                <MenuItem fx:id="SaveTimeline" mnemonicParsing="false" text="Save Timelines" />
              </items>
            </MenuButton>
         </children>
      </AnchorPane>
      <VBox fx:id="vbox_timeline" prefHeight="433.0" prefWidth="660.0">
         <children>
            <ScrollPane prefHeight="430.0" prefWidth="660.0">
              <content>
                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="200.0" prefWidth="200.0" />
              </content>
            </ScrollPane>
         </children>
      </VBox>
      
   </children>
</VBox>
