<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.MenuButton?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox fx:id="VboxLand" prefHeight="499.0" prefWidth="1000" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.MainController">
   <children>
      <AnchorPane fx:id="AnchorPane">
         <children>
            <MenuButton fx:id="Timeline" layoutX="24.0" layoutY="24.0" mnemonicParsing="false" prefHeight="45.0" prefWidth="118.0" text="Timeline">
              <items>
                <MenuItem fx:id="AddTimeline" mnemonicParsing="false" onAction="#appearFormTimeline" text="Add Timeline" />
                <MenuItem  mnemonicParsing="false" onAction="#loadTimelines" text="Load Timelines" />
                <MenuItem  mnemonicParsing="false" onAction="#saveTimelines" text="Save Timelines" />
             </items>
            </MenuButton>
         </children>
      </AnchorPane>
   </children>
   <children>
     <ScrollPane fx:id="mainScrollPane" >
          <content>
              <VBox fx:id="vBoxModules" prefHeight="800.0" prefWidth="1000"   >
                  <children>
                  </children>
              </VBox>
          </content>
     </ScrollPane>
     </children>
</VBox>
